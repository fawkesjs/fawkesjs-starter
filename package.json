{
  "name": "fawkesjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "swagger": "fawkesjs -s ./swagger/swagger.json",
    "roll": "ts-node src/bin/roll.ts",
    "db:sync": "ts-node src/bin/sync.ts",
    "db:migrate": "sequelize --config ./app/config/sequelize.js db:migrate",
    "db:migrate:undo": "sequelize --config ./app/config/sequelize.js db:migrate:undo"
  },
  "license": "MIT",
  "author": "nghenglim",
  "dependencies": {
    "bcrypt-as-promised": "^1.1.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1",
    "fawkesjs": "file:../fawkesjs",
    "glob": "^5.0.14",
    "i18n": "^0.8.3",
    "lodash": "^3.10.1",
    "mongoose": "^4.1.5",
    "morgan": "~1.6.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.29.0",
    "sequelize-cli": "^2.5.1",
    "typescript": "^2.1.5",
    "uuid": "^3.0.1",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.5"
  }
}
