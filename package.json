    "bcrypt-as-promised": "^1.1.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "^1.3.5",
    "ejs": "^2.5.6",
    "express": "~4.13.1",
    "fawkesjs": "~0.5.5",
    "glob": "^5.0.14",
    "i18n": "^0.8.3",
    "lodash": "^3.10.1",
    "mongoose": "^4.1.5",
    "morgan": "~1.6.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.29.0",
    "sequelize-cli": "^2.5.1",
    "sqlite3": "^3.1.8",
    "typescript": "^2.3.2",
    "uuid": "^3.0.1",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "pre-push": "^0.1.1",
    "tslint": "^5.1.0"
  },
