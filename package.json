{
  "name": "fawkesjs-starter",
  "version": "0.5.5",
  "main": "index.js",
  "scripts": {
    "start": "tsc && fawkesjs -s ./swagger/swagger.json && node app/bin/sync.js && node .",
    "dev": "tsc && fawkesjs -s ./swagger/swagger.json && node app/bin/sync.js && nodemon ./index.js -e js --watch app",
